{
    "name": "drrajshah-11ty",
    "version": "1.0.0",
    "description": "Eleventy site migrated from WordPress",
    "scripts": {
        "start": "ELEVENTY_ENV=development npx @11ty/eleventy --serve",
        "build": "eleventy",
        "build:sass:prod": "sass src/assets/css:public/static/css --style compressed",
        "build:eleventy:prod": "ELEVENTY_ENV=production eleventy",
        "build:prod": "npm run build:eleventy:prod & npm run build:sass:prod",
        "dev": "npm run start",
        "import:wp": "node scripts/import-wp.js ./wordpress-export.xml"
    },
    "dependencies": {
        "@11ty/eleventy": "^3.1.2",
        "@11ty/eleventy-navigation": "^0.3.5",
        "@11ty/eleventy-plugin-rss": "^2.0.4",
        "markdown-it": "^13.0.0"
    },
    "devDependencies": {
        "fs-extra": "^11.1.1",
        "sass": "^1.91.0",
        "slugify": "^1.6.5",
        "turndown": "^7.1.1",
        "xml2js": "^0.4.23"
    }
}
